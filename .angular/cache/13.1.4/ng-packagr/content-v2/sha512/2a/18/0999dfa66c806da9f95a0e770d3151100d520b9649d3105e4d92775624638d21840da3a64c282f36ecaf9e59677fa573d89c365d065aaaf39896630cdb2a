Of(this.mask[i]) === -1) {\r\n                chars[this.mask[i]] = 1;\r\n            }\r\n            else {\r\n                chars[this.mask[i]]++;\r\n            }\r\n            if (chars[this.mask[i]] > max) {\r\n                max = chars[this.mask[i]];\r\n                maskChar = this.mask[i];\r\n            }\r\n        }\r\n        return maskChar;\r\n    }\r\n    getCursorPosition() {\r\n        return this.el.nativeElement.selectionStart;\r\n    }\r\n    setCursorPosition(start, end = start) {\r\n        this.el.nativeElement.setSelectionRange(start, end);\r\n    }\r\n    getCode(event) {\r\n        return event.keyCode || event.charCode;\r\n    }\r\n}\r\nMaskDirective.ɵfac = function MaskDirective_Factory(t) { return new (t || MaskDirective)(i0.ɵɵdirectiveInject(i0.ElementRef), i0.ɵɵdirectiveInject(i2.NgControl)); };\r\nMaskDirective.ɵdir = /*@__PURE__*/ i0.ɵɵdefineDirective({ type: MaskDirective, selectors: [[\"input\", \"mask\", \"\"]], hostBindings: function MaskDirective_HostBindings(rf, ctx) { if (rf & 1) {\r\n        i0.ɵɵlistener(\"keydown\", function MaskDirective_keydown_HostBindingHandler($event) { return ctx.onKeyDown($event); })(\"keyup\", function MaskDirective_keyup_HostBindingHandler($event) { return ctx.onKeyUp($event); });\r\n    } }, inputs: { mask: \"mask\" } });\r\n(function () { (typeof ngDevMode === \"undefined\" || ngDevMode) && i0.ɵsetClassMetadata(MaskDirective, [{\r\n        type: Directive,\r\n        args: [{\r\n                selector: 'input[mask]'\r\n            }]\r\n    }], function () { return [{ type: i0.ElementRef }, { type: i2.NgControl }]; }, { mask: [{\r\n            type: Input\r\n        }], onKeyDown: [{\r\n            type: HostListener,\r\n            args: ['keydown', ['$event']]\r\n        }], onKeyUp: [{\r\n            type: HostListener,\r\n            args: ['keyup', ['$event']]\r\n        }] }); })();\n\nfunction VMaskedFieldComponent_mat_label_1_Template(rf, ctx) { if (rf & 1) {\r\n    i0.ɵɵelement